<HWLOG>										<!-- Setting for hardware manager log 	-->
	<path>.\\logs\\</path>	  					<!-- name of path of logfile  (backslash)	-->
	<filename>hardware</filename>		  		<!-- name of log file 			-->
	<type>0</type>	  							<!-- 0 = rotating 1 = stdout	-->
	<filesize>250</filesize>  					<!-- filesize in KByte  		-->
	<maxfileno>5</maxfileno>	  				<!-- max no of filse 			-->
	<forceflush>1</forceflush>	  				<!-- 0 = OFF (file is written after closing app ) 1 = ON 			-->
</HWLOG>

